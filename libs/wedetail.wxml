
<template name="selecticon1">
  <view class="weui-cell__hd">
    <image src="data:image/png;base64," style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
  </view>
</template>

<template name="selecticon2">
  <view class="weui-cell__hd">
    <image src="data:image/png;base64," style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
  </view>
</template>

<template name="-1">
  <view class="weui-cell__hd">
      <image src="{{c.thumbnail}}" class="weui-thumb"></image>
  </view>
  <view class="weui-cell__bd">{{c.uName}}</view>
  <view class="weui-cell__ft weui-cell__ft_in-access">{{c.title}}</view>
</template>
<template name="-2">
  <image src="{{c}}" mode="aspectFit"></image>
</template>
<template name="-3">
  <audio src="{{c}}"></audio>
</template>
<template name="-4">
  <video src="{{c}}"></video>
</template>
<template name="-5">
  <map longitude="{{c.longitude}}" latitude="{{c.latitude}}" scale="14" show-location style="width: 100%; height: 300px;"></map>
</template>
<template name="-6">
  <file src="{{c}}"></file>
</template>
